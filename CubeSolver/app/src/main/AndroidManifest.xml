<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jlevy.cubesolver">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Cube Solver"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CapturePreview">
        </activity>
        <activity android:name=".InstructionsActivity">
        </activity>
        <activity android:name=".GuidelineMoverActivity">
        </activity>
        <activity android:name=".CubePainterActivity">
        </activity>
        <activity android:name=".CubeSolverActivity">
        </activity>
    </application>

</manifest>
